{
	"name": "dev-boon",
	"displayName": "dev-boon",
	"description": "An extension which helps developers in order to find solutions to errors occured during execution of commands(in command line) through stackOverFlow,youtube videos etc.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/chirag2706/dev-boon.git"
	},
	"publisher": "ChiragGupta",
	"activationEvents": [
		"onCommand:dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT",
		"onCommand:dev-boon.DEACTIVATE_EXTENSION",
		"onCommand:dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT",
		"onCommand:dev-boon.CUSTOM_SEARCH",
		"onCommand:dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT_USING_PROMPT",
		"onCommand:dev-boon.NLP_TO_CODE",
		"onCommand:dev-boon.CODE_SUMMARY",
		"onView:dev-boon-sidebar"
	],
	"main": "./out/extension.js",
	"license": "MIT",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dev-boon-sidebar-view",
					"title": "dev-boon sidenavbar",
					"icon": "icons/sideBarIcon.svg"
				}
			]
		},
		"views": {
			"dev-boon-sidebar-view": [
				{
					"type": "webview",
					"id": "dev-boon-sidebar",
					"name": "dev-boon",
					"icon": "icons/sideBarIcon.svg",
					"contextualTitle": "dev-boon"
				}
			]
		},
		"commands": [
			{
				"command": "dev-boon.NLP_TO_CODE",
				"category": "dev-boon",
				"title": "snippet query"
			},
			{
				"command": "dev-boon.DEACTIVATE_EXTENSION",
				"category": "dev-boon",
				"title": "deactivate dev-boon extension"
			},
			{
				"command": "dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT",
				"category": "dev-boon",
				"title": "StackOverFlow Search"
			},
			{
				"command": "dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT",
				"category": "dev-boon",
				"title": "YouTube Search"
			},
			{
				"command": "dev-boon.CUSTOM_SEARCH",
				"category": "dev-boon",
				"title": "Search using prompt"
			},
			{
				"command": "dev-boon.CODE_SUMMARY",
				"category": "dev-boon",
				"title": "code summary"
			}
		],
		"keybindings": [
			{
				"command": "dev-boon.NLP_TO_CODE",
				"key": "ctrl+q"
			},
			{
				"command": "dev-boon.STACKOVERFLOW_SEARCH_WITH_SELECTED_TEXT",
				"key": "ctrl+shift+s"
			},
			{
				"command": "dev-boon.YOUTUBE_SEARCH_WITH_SELECTED_TEXT",
				"key": "ctrl+shift+y"
			},
			{
				"command": "dev-boon.DEACTIVATE_EXTENSION",
				"key": "ctrl+shift+d"
			},
			{
				"command": "dev-boon.CUSTOM_SEARCH",
				"key": "ctrl+shift+q"
			},
			{
				"command": "dev-boon.CODE_SUMMARY",
				"key": "ctrl+shift+t"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/request-promise-native": "^1.0.17",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@rollup/plugin-typescript": "^8.2.0",
		"@tsconfig/svelte": "^1.0.10",
		"axios": "^0.21.1",
		"bootstrap": "^5.0.0-beta2",
		"child-process": "^1.0.2",
		"concurrently": "^6.0.0",
		"cross-spawn": "^7.0.3",
		"express": "^4.17.1",
		"fs": "0.0.1-security",
		"open": "^8.0.1",
		"python-shell": "^2.0.3",
		"request": "^2.88.2",
		"request-promise-native": "^1.0.9",
		"rollup": "^2.40.0",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.35.0",
		"svelte-check": "^1.2.1",
		"svelte-preprocess": "^4.6.9"
	}
}
